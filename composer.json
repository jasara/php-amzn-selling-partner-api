{
    "name": "jasara/php-amzn-selling-partner-api",
    "description": "A fluent interface for Amazon's Selling Partner API in PHP",
    "type": "library",
    "license": "CC-BY-NC-SA-4.0",
    "authors": [
        {
            "name": "Jasara Technology Inc.",
            "email": "keith@jasaratech.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Jasara\\AmznSPA\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Jasara\\AmznSPA\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.4",
        "aws/aws-sdk-php": "^3.199",
        "illuminate/http": "^10.0 || ^11.0 || ^12.0",
        "vlucas/phpdotenv": "^5.3",
        "brick/money": "^0.5 || ^0.6 || ^0.7 || ^0.8 || ^0.9 || ^0.10",
        "illuminate/support": "^10.0 || ^11.0 || ^12.0",
        "illuminate/validation": "^10.0 || ^11.0 || ^12.0",
        "php-units-of-measure/php-units-of-measure": "^2.1",
        "psr/log": "^1.0.1 || ^2.0 || ^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0",
        "spatie/ray": "^1.30",
        "laravel/pint": "^1.10",
        "rector/rector": "^2.0",
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "friendsofphp/php-cs-fixer": "^3.75"
    }
}
